  %body
    .contact-page
      @@include('./_header.html')


        %section.contact
          .contact-headline
            %h3 Contact
          .contact-description
            .container
              .row
                .col-md-5
                  .main-form-wrapper
                    %h3 Salut!
                    %p Cu ce te putem ajuta?
                    %form.main-form.go-right
                      .form-group
                        %input#name.form-control(name="name" required="" type="text" placeholder="Nume" )
                        %label.form-label(name="name") Nume
                      .form-group
                        %input#email.form-control(name="email" required="" type="email" placeholder="E-mail" )
                        %label.form-label(for="email") E-mail
                      .form-group
                        %input#phone.form-control(name="phone" required="" type="phone" placeholder="Număr telefon" )
                        %label.form-label(for="phone") Telefon

                      .form-group
                        %textarea#message.form-control(name="phone" required="" rows="3" placeholder="As dori mai multe detalii referitoare la.." )
                        %label.form-label(for="message") Mesaj
                      .form-group.text-center
                        %label.uploadbutton
                          .button Atașează fișier
                          %input.input-hide(type="file")
                      .form-group
                        .input-group
                          %input.form-control.search-input(type="text")
                          %span.input-group-btn
                            %button.btn.btn-default(type="button") Caută
                      .form-group.text-center
                        %button.send-button Trimite

