{% block html_head_container %}

    {% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    </head>
{% endblock %}

<body class="{{ body_class }}" data-template="base.twig">
<header>
    {% block header %}
        <div class="header-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <p class="partners-description">Acest site este cofinanţat din Fondul Social European prin
                            Programul Operaţional Sectorial Dezvoltarea Resurselor Umane 2007-2013</p>
                        <div class="logo-partners"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="main-header">
            <div class="container">
                <div class="row">
                    <div class="col-md-2 col-xs-4">
                        <a href="{{ site.url }}" rel="home" title="{{ site.name }}" class="logo"></a>
                    </div>
                    <div class="col-md-3 col-md-offset-1 col-xs-6">
                        <div class="input-mobile-wrapper">
                            <div class="input-group input-group-head">
                                <input placeholder="Adresa de e-mail" type="email" class="form-control"/>
                                <span class="input-group-btn">
                                    <button type="button" class="btn btn-default">Caută</button>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-5 text-right mob-hide">
                        <ul class="main-nav">
                            <li><a href="{{ getPermalink('promote-yourself') }}">{{ __('POMOVEAZĂ-TE', 'project') }}</a>
                            </li>
                            <li><a href="{{ getPermalink('blog') }}">{{ __('ŞTIRI', 'project') }}</a>
                            </li>
                            {% if fn('is_user_logged_in') %}
                                <li class="dropdown-block save-block">
                                    <a href="{{ getPermalink('my-account') }}"
                                       class="dropdown-item icon-inimacasa-01"></a>
                                    <ul class="header-menu-dropdown">
                                        <li><a href="{{ getPermalink('my-account') }}" class="dropdown-link">
                                                {{ __('Salvează căutările', 'project') }}
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            {% endif %}
                            <li>
                                <a href="{{ getPermalink('contacts') }}">{{ __('CONTACT', 'project') }}</a>
                            </li>
                            <li>
                                {% if fn('is_user_logged_in') %}
                                    <a href="{{ getPermalink('my-account') }}">{{ __('Profilul meu', 'project') }}</a>
                                {% else %}
                                    <a href="{{ getPermalink('login') }}">{{ __('LOGIN', 'project') }}</a>
                                {% endif %}
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-1 mob-hide">
                        <ul class="header-menu">
                            <li class="dropdown-block">
                                <a href="#" class="dropdown-item">{{ __('MENIU', 'project') }}</a>
                                <ul class="header-menu-dropdown">
                                    {% if header_menu is not empty %}
                                        {% include 'partials/header_menu_items.twig' with {'menu': header_menu} %}
                                    {% endif %}
                                    <li><a href="#" class="dropdown-link dropdown-link-last show-modal-form">{{ __('Părerea voastră', 'project') }}</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-6 col-xs-2">
                        <div class="navbar-menu-overlay"></div>
                        <a href="#" class="mob-menu-icon"><span></span></a>
                        <div class="mob-sidebar">
                            <div class="mob-menu-container">
                                <ul class="menu menu-mob">

                                    <li>
                                        <a href="{{ getPermalink('promote-yourself') }}" class="dropdown-link">{{ __('POMOVEAZĂ-TE', 'project') }}</a>
                                    </li>
                                    <li>
                                        <a href="{{ getPermalink('blog') }}" class="dropdown-link">{{ __('ŞTIRI', 'project') }}</a>
                                    </li>
                                    <li>
                                        <a href="{{ getPermalink('contacts') }}" class="dropdown-link">{{ __('CONTACT', 'project') }}</a>
                                    </li>
                                    <li>
                                        {% if fn('is_user_logged_in') %}
                                            <a href="{{ getPermalink('my-account') }}">{{ __('Profilul meu', 'project') }}</a>
                                        {% else %}
                                            <a href="{{ getPermalink('login') }}">{{ __('LOGIN', 'project') }}</a>
                                        {% endif %}
                                    </li>
                                    <li>
                                        <a href="{{ site.url }}" class="dropdown-link">{{ __('Acasă', 'project') }}</a>
                                    </li>
                                    {% include 'partials/header_menu_items.twig' with {'menu': header_menu} %}
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="izi-modal" id="modal-form-window">
            <div class="main-form-wrapper">
                <div class="form-slick">
                    <div class="first-form">
                        <div class="title">
                            <p>Spuneţi-ne despre experienţa cu noi</p>
                            <p>Părerea voastră ne va ajuta să fim mai buni</p>
                            <p>Ai recomanda Vrei casă.ro prietenilor tăi?</p>
                        </div>
                        <div class="modal-form">
                            <div data-attr="1" class="radio-container">
                                <input type="radio" class="radio" id="radio-1" name="radio-btn-1" value="1" />
                                <label for="radio-1" class="bg-color-1"></label><span>1</span>
                            </div>
                            <div data-attr="2" class="radio-container">
                                <input type="radio" class="radio" id="radio-2" name="radio-btn-1" value="2" />
                                <label for="radio-2" class="bg-color-2"></label><span>2</span>
                            </div>
                            <div data-attr="3" class="radio-container">
                                <input type="radio" class="radio" id="radio-3" name="radio-btn-1" value="3" />
                                <label for="radio-3" class="bg-color-3"></label><span>3</span>
                            </div>
                            <div data-attr="4" class="radio-container">
                                <input type="radio" class="radio" id="radio-4" name="radio-btn-1" value="4" />
                                <label for="radio-4" class="bg-color-4"></label><span>4</span>
                            </div>
                            <div data-attr="5" class="radio-container">
                                <input type="radio" class="radio" id="radio-5" name="radio-btn-1" value="5" />
                                <label for="radio-5" class="bg-color-5"></label><span>5</span>
                            </div>
                        </div>
                        <div class="result-rating">
                            <div data-attr="1" class="result">
                                <div class="img-container">
                                    <img src="{{theme.link}}/assets/images/smile-5.png" />
                                </div>
                                <div class="gray-letter">
                                    <p>În nici un caz</p>
                                </div>
                            </div>
                            <div data-attr="2" class="result">
                                <div class="img-container">
                                    <img src="{{theme.link}}/assets/images/smile-4.png" />
                                </div>
                                <div class="gray-letter">
                                    <p>Nu încă</p>
                                </div>
                            </div>
                            <div data-attr="3" class="result">
                                <div class="img-container">
                                    <img src="{{theme.link}}/assets/images/smile-3.png" />
                                </div>
                                <div class="gray-letter">
                                    <p>Da, o să recomand</p>
                                </div>
                            </div>
                            <div data-attr="4" class="result">
                                <div class="img-container">
                                    <img src="{{theme.link}}/assets/images/smile-2.png" />
                                </div>
                                <div class="gray-letter">
                                    <p>În fine</p>
                                </div>
                            </div>
                            <div data-attr="5" class="result">
                                <div class="img-container">
                                    <img src="{{theme.link}}/assets/images/smile.png" />
                                </div>
                                <div class="gray-letter">
                                    <p>Deja am recomandat</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="second-form">
                        <div class="title">
                            <p>Cum ai aflat de Vrei casa.ro</p>
                        </div>
                        <div class="modal-form">
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-6" name="radio-btn-2" value="6" />
                                <label for="radio-6"></label><span>Pe internet</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-7" name="radio-btn-2" value="7" />
                                <label for="radio-7"></label><span>Reviste, pliante</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-8" name="radio-btn-2" value="8" />
                                <label for="radio-8"></label><span>Banere, postere</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-9" name="radio-btn-2" value="4" />
                                <label for="radio-9"></label><span>De la prieteni sa familie</span>
                            </div>
                        </div>
                    </div>
                    <div class="third-form">
                        <div class="title">
                            <p>Folosirea Vrei casă.ro este:</p>
                        </div>
                        <div class="modal-form">
                            <div class="radio-container">
                                <input type="radio" class="radio orange-col" id="radio-10" name="radio-btn-3" value="9" />
                                <label for="radio-10"></label><span>Greu</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio violet-col" id="radio-11" name="radio-btn-3" value="10" />
                                <label for="radio-11"></label><span class="pos-left">În regulă</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio pink-col" id="radio-12" name="radio-btn-3" value="11" />
                                <label for="radio-12"></label><span>Uşor</span>
                            </div>
                        </div>
                    </div>
                    <div class="last-form">
                        <div class="title">
                            <p>Aveţi sfaturi pentru noi?</p>
                        </div>
                        <div class="modal-form">
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-13" name="radio-btn-4" value="12" />
                                <label for="radio-13"></label><span>Pozele pot fi mai bune</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-14" name="radio-btn-4" value="13" />
                                <label for="radio-14"></label><span>Necesita mai multe informaţii referitoare la amplasament</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-15" name="radio-btn-4" value="14" />
                                <label for="radio-15"></label><span>Postaţi mai multe oferte</span>
                            </div>
                            <div class="radio-container">
                                <input type="radio" class="radio" id="radio-16" name="radio-btn-4" value="15" />
                                <label for="radio-16"></label><span>Rectificaţi informaţiile greşite</span>
                            </div>
                            <div class="btn-container">
                                <button type="button" class="alt">Alt sfat</button>
                                <button type="submit" class="btn-subm">Trimite</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {% endblock %}
</header>
<div class="main-container">
    {% block content %}
        Sorry, no content
    {% endblock %}
</div>
{% block footer %}
    <footer class="footer">
        {% include 'footer.twig' %}
    </footer>
